<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--CustomToolbar-->
    <declare-styleable name="CustomToolbar">
        <attr name="ct_title" format="string"/>
        <attr name="ct_navigationType" format="enum">
            <enum name="none" value="0"/>
            <enum name="back" value="1"/>
            <enum name="menu" value="2"/>
            <enum name="close" value="3"/>
            <enum name="home" value="4"/>
        </attr>
    </declare-styleable>
</resources>